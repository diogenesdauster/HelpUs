<Alloy>
	<Tab id="institutosTab">
		<Window>
			<TableView onClick="showDetails" id="myTable" dataCollection="institutos" >
				<TableViewRow  identificador="{alloy_id}" >
					<View id="view">
						<ImageView id="image" image="{img}" />
						<View id="viewDados">
							<Label id="name" text="{nome}"/>
						</View>
					</View>
				</TableViewRow>
			</TableView>
		</Window>
	</Tab>
</Alloy>